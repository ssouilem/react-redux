{
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "babel-node ./bin/compile",
    "compile": "better-npm-run compile",
    "dev": "better-npm-run dev",
    "langs:build": "NODE_TLS_REJECT_UNAUTHORIZED=0 babel-node intl/translate/index.js",
    "langs:test": "babel-node ./node_modules/mocha/bin/mocha --compilers js:babel-register  -R xunit intl/translate/tests/translate/translate.spec.js | grep -v \"missing\" | grep -v \"Duplicate\" > test-reports.xml",
    "test": "better-npm-run test",
    "test:dev": "better-npm-run test:dev",
    "generate": "plop --plopfile generators/index.js"
  },
  "betterScripts": {
    "compile": {
      "command": "npm run clean && npm run build",
      "env": {
        "DEBUG": "app:*",
        "DIRECTORY": "frontend",
        "NODE_ENV": "production",
        "API_SERVER": "$API_SERVER",
        "IFRAME_URL": "$IFRAME_URL"
      }
    },
    "dev": {
      "command":
        "nodemon --exec babel-node bin/server --dops_server_host=my-local-dev-env.dops.open.global:7777 --open --inline --hot",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development",
        "HOST": "my-local-dev-env.dops.open.global"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "DEBUG": "app:*",
        "DIRECTORY": "frontend",
        "NODE_ENV": "test"
      }
    },
    "test:dev": {
      "command": "babel-node ./node_modules/karma/bin/karma start config/karma.config --watch",
      "env": {
        "DEBUG": "app:*",
        "DIRECTORY": "frontend",
        "NODE_ENV": "test"
      }
    }
  },
